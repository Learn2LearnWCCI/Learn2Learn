@model IEnumerable<Learn2Learn.Models.Results>

@{
    ViewBag.Title = "Results";
}

<h2>Assessment Results</h2>
<p>
    @Html.ActionLink("Take Asessment Again", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AuditoryResult)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VisualResult)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KinestheticResult)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AuditoryResult)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VisualResult)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.KinestheticResult)
        </td>
        <td>
            @*@Html.ActionLink("Edit", "Edit", new { id=item.ID }) | not allowing edit*@
            @Html.ActionLink("Details", "Details", new { id=item.ID }) 
            @*@Html.ActionLink("Delete", "Delete", new { id=item.ID }) not allowing delete*@
        </td>
    </tr>
}

</table>
